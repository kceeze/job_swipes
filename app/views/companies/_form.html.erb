<%= form_for @company do |f| %>
	
  <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
  </div>

  <div class="field">
      <%= f.label :street %>
      <%= f.text_field :street %>
  </div>


  <div class="field">
      <%= f.label :city %>
      <%= f.text_field :city %>
  </div>

  <div class="field">
      <%= f.label :state %>
      <%= f.text_field :state %>
  </div>


  <div class="field">
      <%= f.label :zip %>
      <%= f.text_field :zip %>
  </div>


 <div class="actions">
    <%= f.submit "Submit" %>
  </div>
<% end %>